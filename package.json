{
  "name": "angular2-highcharts",
  "version": "0.1.1",
  "description": "Highcharts for your Angular2 project",
  "scripts": {
    "watch": "ntsc -w",
    "build": "rm -rf dist && node ./node_modules/typescript/bin/tsc",
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "typings-install" : "node ./node_modules/typings/dist/bin install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gevgeny/angular2-highcharts"
  },
  "keywords": [
    "Angular2",
    "ng2",
    "charts",
    "highcharts"
  ],
  "author": "Eugene Gluhotorenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gevgeny/angular2-highcharts/issues"
  },
  "homepage": "https://github.com/gevgeny/angular2-highcharts#readme",
  "devDependencies": {
    "@angular/http": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/common": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/platform-server": "2.0.0-rc.3",

    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "reflect-metadata": "^0.1.3",
    "http-server": "0.8.5",
    "jasmine-core": "2.4.1",
    "typescript": "^1.8.0",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-jasmine": "0.3.6",
    "typings": "^1.0.4",
    "systemjs": "0.19.17",
    "es6-shim": "0.35.0"
  },
  "dependencies": {
    "highcharts": "^4.2.1"
  }
}
